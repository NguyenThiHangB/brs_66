<% provide :title, t(".title") %>
<h1><%= t ".ls_au" %></h1>
<div class="form-group">
  <%= link_to t(".create"), new_admin_author_path, class: "btn btn-info" %>
</div>
<%= paginate @authors, theme: "twitter-bootstrap-3"%>
<table class="table table-bordered">
  <thead>
    <tr>
      <th><%= t ".stt" %></th>
      <th><%= t ".name" %></th>
      <th><%= t ".date_of_birth" %></th>
      <th><%= t ".phone" %></th>
      <th><%= t ".address" %></th>
      <th class="table-th" colspan="2" ><%= t ".active" %></th>
    </tr>
  </thead>
  <tbody>
    <%= render "show_list" %>
  </tbody>
</table>
<%= paginate @authors, theme: "twitter-bootstrap-3" %>
